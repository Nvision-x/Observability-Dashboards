{
  "id": null,
  "uid": "nx-applications-monitoring",
  "title": "NX Applications Monitoring",
  "tags": ["nx-applications", "monitoring", "observability", "git-sync"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "templating": {
    "list": [
      {
        "name": "namespace",
        "type": "query",
        "datasource": "Prometheus",
        "refresh": 1,
        "query": "label_values(kube_pod_info, namespace)",
        "current": {
          "text": "default",
          "value": "default"
        }
      }
    ]
  },
  "panels": [
    {
      "type": "graph",
      "title": "alerts-api CPU Usage",
      "gridPos": { "x": 0, "y": 0, "w": 6, "h": 8 },
      "targets": [
        {
          "expr": "sum(rate(container_cpu_usage_seconds_total{namespace='$namespace', pod=~'.*alerts-api.*'}[5m]))",
          "legendFormat": "alerts-api",
          "refId": "A"
        }
      ]
    },
    {
      "type": "graph",
      "title": "alerts-api Memory Usage",
      "gridPos": { "x": 6, "y": 0, "w": 6, "h": 8 },
      "targets": [
        {
          "expr": "sum(container_memory_usage_bytes{namespace='$namespace', pod=~'.*alerts-api.*'})",
          "legendFormat": "alerts-api",
          "refId": "B"
        }
      ]
    },
    {
  "type": "table",
  "title": "alerts-api POD Status",
  "gridPos": { "x": 12, "y": 0, "w": 6, "h": 8 },
  "targets": [
    {
      "expr": "kube_pod_status_ready{namespace=\"default\",pod=~\"applications-alerts-api.*\"}",
      "legendFormat": "{{pod}}",
      "refId": "C"
    }
  ],
  "options": {
    "showHeader": true
  },
  "fieldConfig": {
    "defaults": {
      "unit": "none",
      "thresholds": {
        "mode": "absolute",
        "steps": [
          { "color": "red", "value": 0 },
          { "color": "green", "value": 1 }
        ]
      }
    },
    "overrides": []
  }
},
